func fib_memo(n: int, a: int, b: int,): int {
    if (n == 0) {
        return a;
    }

    if (n == 1) {
        return b;
    }

    const count: int = n - 1;
    const next: int = a + b;

    return fib_memo(count, b, next,);
}

func main(): int {
    const ans: int = fib_memo(4, 1, 1,);

    if (ans != 5) {
        return 1;
    }

    return 0;
}
