use func printInt(x: int,): int;

func sum_n(n: int,): int {
    let temp: int = n;
    let ans: int = 0;

    while (temp > 0) {
        ans = ans + temp;
        temp = temp - 1;
    }

    return ans;
}

func main(): int {
    const answer: int = sum_n(10,);
    printInt(answer,);

    if (answer != 55) {
        return 1;
    }

    return 0;
}